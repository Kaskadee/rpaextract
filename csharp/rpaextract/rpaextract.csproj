<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <StartupObject>rpaextract.Program</StartupObject>
    <AssemblyName>rpaextract</AssemblyName>
    <RootNamespace>rpaextract</RootNamespace>
    <AssemblyVersion>0.2.0</AssemblyVersion>
    <FileVersion>0.2.0</FileVersion>
    <Copyright>Copyright (c) 2017-2018 Fabian Creutz</Copyright>
    <PackageLicenseUrl>https://github.com/Kaskadee/rpaextract/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Kaskadee/rpaextract/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Kaskadee/rpaextract/</RepositoryUrl>
    <Version>0.2.0</Version>
    <Description>An application for listing/extracting content from Ren'py archives.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SharpCompress" Version="0.23.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\sharppickle\sharppickle\sharppickle.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update="LICENSE.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
